{
  "name": "etaraz-web-bp",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.test.conf.json --port 4300",
    "start-prod": "ng serve --proxy-config proxy.conf.json --port 4300",
    "build": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --prod",
    "build-test": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --configuration=test",
    "build-deploy-prod": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --prod && npm run robots-copy-prod",
    "build-deploy-test": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --configuration=test && npm run robots-copy-test",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "robots-copy-test": "shx cp src/robots/robots.test.txt /var/www/html/robots.txt",
    "robots-copy-prod": "shx cp src/robots/robots.prod.txt /var/www/html/robots.txt"
  },
  "private": true,
